<div class="bb-full-width">
    <div class="bb-contact-container">
        <div class="bb-full-width bb-contact-box">
            <div class="bb-full-width text-center">
                <h2 class="head-welcome bb-full-width">Contact Us</h2>
            </div>
            <div class="bb-full-width">
                @using (Html.BeginForm("ContactUs", "Home", FormMethod.Post, htmlAttributes: new
                { @class = "bb-full-width" }))
                {
                    <div class="form-group">
                        <label class="bb-label-contact" for="">Name</label>
                        @*<input type="text" class="form-control bb-input-contact" id="" placeholder="Enter Your Name" required="">*@
                        @Html.TextBoxFor(m => m.Name, null, htmlAttributes: new { @class = "form-control bb-input-contact", @placeholder = "Enter your name" })
                        @Html.ValidationMessageFor(m => m.Name, "", new { @class = "text-danger" })
                    </div>
                    <div class="form-group">
                        <label class="bb-label-contact" for="">Contact</label>
                        @*<input type="text" class="form-control bb-input-contact" id="" placeholder="Enter Your Contact" required="">*@
                        @Html.TextBoxFor(m => m.Contact, null, htmlAttributes: new { @class = "form-control bb-input-contact", @placeholder = "Enter your mobile number" })
                        @Html.ValidationMessageFor(m => m.Contact, "", new { @class = "text-danger" })
                    </div>
                    <div class="form-group">
                        <label class="bb-label-contact" for="">Email</label>
                        @*<input type="email" class="form-control bb-input-contact" id="" placeholder="Enter Your Email" required="">*@
                        @Html.TextBoxFor(m => m.Email, null, htmlAttributes: new { @class = "form-control bb-input-contact", @placeholder = "Enter your email" })
                        @Html.ValidationMessageFor(m => m.Email, "", new { @class = "text-danger" })
                    </div>
                    <div class="form-group">
                        <label class="bb-label-contact" for="">Message</label>
                        @*<textarea class="form-control bb-input-contact" placeholder="Type Your Message..." required="" width="100%" rows="5"></textarea>*@
                        @Html.TextAreaFor(m => m.Message, htmlAttributes: new { @class = "form-control bb-input-contact", @placeholder = "Enter your message" })
                        @Html.ValidationMessageFor(m => m.Message, "", new { @class = "text-danger" })
                    </div>
                    <div class="form-group">
                        <input type="submit" class="bb-full-width btn bb-contact-form-submit" id="">
                    </div>

                }

            </div>

            <div class="bb-contact-box-saprator text-center">
                <span class="">Or Connect With</span>
            </div>

